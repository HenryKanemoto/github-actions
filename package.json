{
  "dependencies": {
    "next": "^14.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rimraf build && next build && next export && move out build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "npm run lint --fix",
    "deploy": "npm build && npx vercel ./build --confirm --no-clipboard",
    "deploy:prod": "npm run build && vercel ./build --confirm --no-clipboard --prod --token=$VERCEL_TOKEN",
    "deploy:preview": "node ./scripts/pullRequestPreview.js"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1"
  }
}
